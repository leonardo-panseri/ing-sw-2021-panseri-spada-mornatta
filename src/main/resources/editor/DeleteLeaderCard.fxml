<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root type="VBox" prefHeight="300.0" prefWidth="400.0" fx:controller="it.polimi.ingsw.editor.controller.DeleteLeaderCard"
      xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
   <AnchorPane prefHeight="300.0" prefWidth="400.0">
      <Button layoutX="90.0" layoutY="150.0" mnemonicParsing="false" prefHeight="63.0" prefWidth="80.0" text="Yes"
              onAction="#accept"/>
      <Button layoutX="215.0" layoutY="150.0" mnemonicParsing="false" prefHeight="63.0" prefWidth="80.0" text="No"
              onAction="#refuse"/>
      <Label layoutX="80.0" layoutY="51.0" prefHeight="63.0" prefWidth="228.0"
             text="Are you sure you want to delete this card?"/>
   </AnchorPane>
</fx:root>
